l=list(map(int,input().split()))[:2]
n=l[0]
m=l[1]
for i in range(n//2,0,-1):
    print("".ljust(i*3,'-')+(".|."*(n//2-i+1))+".|."*(n//2-i)+"".rjust(i*3,'-'))
print("WELCOME".center(m,'-'))
for j in range(n//2):
    print("".ljust((j+1)*3,'-')+(".|."*(n//2-j))+".|."*(n//2-j-1)+"".rjust((j+1)*3,'-'))
